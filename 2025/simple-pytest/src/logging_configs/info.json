{
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
        "simple": {
            "format": "\r [%(levelname)s]: %(asctime)s - %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers": [
                "console"
            ]
        }
    }
}